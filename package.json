{
  "name": "aurora",
  "groupId": "no.skatteetaten.aurora",
  "version": "1.0.0",
  "description": "Documentation for The Aurora Platform",
  "license": "Apache-2.0",
  "files": [
    "metadata",
    "public"
  ],
  "scripts": {
    "prestart": "npm run clean",
    "start": "gatsby develop --verbose",
    "prepare": "husky install",
    "prebuild": "npm run clean",
    "build": "GATSBY_CPU_COUNT=7 gatsby build --prefix-paths",
    "build:local": "gatsby build",
    "deploy": "gh-pages -d public -p 0",
    "prettier:fix": "prettier --write '{src,docs}/**/*.{js,jsx,css,md}'",
    "test:coverage": "echo \"no test specified\"",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@skatteetaten/frontend-components": "16.0.1",
    "classnames": "2.5.1",
    "gatsby": "5.14.5",
    "gatsby-plugin-manifest": "5.14.0",
    "gatsby-remark-autolink-headers": "6.14.0",
    "gatsby-remark-copy-linked-files": "6.14.0",
    "gatsby-remark-prismjs": "7.14.0",
    "gatsby-source-filesystem": "5.14.0",
    "gatsby-transformer-remark": "6.14.0",
    "github-slugger": "2.0.0",
    "lodash": "4.17.21",
    "prismjs": "1.29.0",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rehype-react": "7.2.0"
  },
  "devDependencies": {
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "prettier": "3.3.3",
    "pretty-quick": "3.1.3",
    "rimraf": "4.4.1",
    "typescript-plugin-css-modules": "4.2.3"
  },
  "overrides": {
    "http-cache-semantics": "4.1.1",
    "core-js-pure": "3.29.1",
    "ua-parser-js": "0.7.33",
    "eslint-import-resolver-node": "0.3.6",
    "clipboardy@2.3.0": {
      "execa": "2"
    },
    "es5-ext": "0.10.53",
    "webpack": "5.76.2",
    "path-to-regexp": "0.1.12"
  },
  "prettier": {
    "singleQuote": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
