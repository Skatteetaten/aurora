{
  "name": "aurora",
  "groupId": "no.skatteetaten.aurora",
  "version": "1.0.0",
  "description": "Documentation for The Aurora Platform",
  "license": "Apache-2.0",
  "files": [
    "metadata",
    "public"
  ],
  "scripts": {
    "prestart": "npm run clean",
    "start": "gatsby develop --verbose",
    "prepare": "husky install",
    "prebuild": "npm run clean",
    "build": "gatsby build --prefix-paths",
    "build:local": "gatsby build",
    "deploy": "gh-pages -d public -p 0",
    "prettier:fix": "prettier --write '{src,docs}/**/*.{js,jsx,css,md}'",
    "test:coverage": "echo \"no test specified\"",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@skatteetaten/frontend-components": "6.0.3",
    "classnames": "2.3.2",
    "gatsby": "5.4.2",
    "gatsby-plugin-manifest": "5.4.0",
    "gatsby-remark-autolink-headers": "6.4.0",
    "gatsby-remark-copy-linked-files": "6.4.0",
    "gatsby-remark-prismjs": "7.4.0",
    "gatsby-source-filesystem": "5.4.0",
    "gatsby-transformer-remark": "6.4.0",
    "github-slugger": "2.0.0",
    "lodash": "4.17.21",
    "prismjs": "1.29.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-react": "7.1.2"
  },
  "devDependencies": {
    "gh-pages": "4.0.0",
    "husky": "8.0.3",
    "prettier": "2.8.3",
    "pretty-quick": "3.1.3",
    "rimraf": "4.1.1",
    "typescript-plugin-css-modules": "4.1.1"
  },
  "overrides": {
    "eslint-import-resolver-node": "0.3.6",
    "clipboardy@2.3.0": {
      "execa": "2"
    },
    "es5-ext": "0.10.53"
  },
  "prettier": {
    "singleQuote": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}